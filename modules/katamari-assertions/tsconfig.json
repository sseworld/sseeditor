{
  "extends": "../../tsconfig.shared.json",
  "compilerOptions": {
    "outDir": "lib",
    "rootDir": "src",
    "baseUrl": ".",
    "tsBuildInfoFile": "lib/tsconfig.tsbuildinfo",
    "paths": {
      "ssephox/katamari-assertions/test/*": [
        "src/test/ts/module/ssephox/katamari-assertions/test/*"
      ],
      "ssephox/katamari-assertions/demo/*": [
        "src/demo/ts/ssephox/katamari-assertions/demo/*"
      ],
      "ssephox/katamari-assertions/*": [
        "src/main/ts/ssephox/katamari-assertions/*"
      ]
    }
  },
  "include": ["src/demo/ts", "src/main/ts", "src/test/ts"]
}
